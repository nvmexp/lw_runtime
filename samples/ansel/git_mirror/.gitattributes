[attr]utf16 diff -merge -text

# cpp code: LF (\n)
*.c text eol=lf
*.h text eol=lf
*.cpp text eol=lf
*.hpp text eol=lf
*.xml text eol=lf
*.md text eol=lf

# Linux: LF (\n)
pre-commit text eol=lf
packman text eol=lf

# Microsoft: CRLF (\r\n)
*.sln text eol=crlf
*.cmd text eol=crlf
*.vcxproj text eol=crlf

# Non-text
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.mov binary
*.mp4 binary
*.mp3 binary
*.flv binary
*.fla binary
*.swf binary
*.gz binary
*.zip binary
*.7z binary
*.ttf binary
*.pyc binary


