#ifndef _LWDA_CTIME
#define _LWDA_CTIME

#ifndef __LWDACC_RTC__
    #include <time.h>
#else
    typedef long long int time_t;
#endif

#include "detail/__config"

#include "detail/__pragma_push"

#include "detail/libcxx/include/ctime"

#include "detail/__pragma_pop"

#endif //_LWDA_CTIME
